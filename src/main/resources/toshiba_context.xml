<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="toshibaHand" class="ua.javabegin.spring.impl.toshiba.ToshibaHand"/>
    <bean id="toshibaLeg" class="ua.javabegin.spring.impl.toshiba.ToshibaLeg"/>
    <bean id="toshibaHead" class="ua.javabegin.spring.impl.toshiba.ToshibaHead" scope="prototype">
        <qualifier type="ua.javabegin.spring.impl.robot.HeadType" value="ToshibaHead"/>
    </bean>

</beans>